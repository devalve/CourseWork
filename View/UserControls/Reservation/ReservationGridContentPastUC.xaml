<UserControl x:Class="CourseWork.View.UserControls.ReservationGridContentPastUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                 xmlns:local="clr-namespace:CourseWork.View.UserControls" xmlns:viewmodel="clr-namespace:CourseWork.ViewModel" d:DataContext="{d:DesignInstance Type=viewmodel:ReservationCanvasVM}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel x:Name="contentStackPanel">
        <UniformGrid x:Name="contentHeaderGrid"
                         Columns="7"
                DockPanel.Dock="Top" 
                  Height="30" >
          
        </UniformGrid>

        <UniformGrid 
            x:Name="contentGrid"
            Columns="7" 
            Rows="24" Margin="0,20,0,0" 
           >
        </UniformGrid>

    </DockPanel>
</UserControl>
